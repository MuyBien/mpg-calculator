{
  "name": "mpg-calculator",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --reporter=junit --outputFile=junit.xml --coverage.enabled --coverage.provider=istanbul --coverage.all",
    "test:mutation": "npx stryker run",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --format gitlab",
    "build-only": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "vue": "^3.3.10"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.0",
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/vitest-runner": "^8.0.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.3",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.55.0",
    "eslint-formatter-gitlab": "^5.1.0",
    "eslint-plugin-vitest": "^0.3.10",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5",
    "typescript": "~5.3.2",
    "vite": "^5.0.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.24"
  }
}
